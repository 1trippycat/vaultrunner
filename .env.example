# Docker environment configuration for VaultRunner

# Vault Configuration
VAULT_ADDR=http://vault:8200
VAULT_ROOT_TOKEN=myroot
# VAULT_TOKEN=your-vault-token-here
# VAULT_NAMESPACE=your-namespace

# VaultRunner Configuration
VAULTRUNNER_LOG_LEVEL=info
VAULTRUNNER_DRY_RUN=false
VAULTRUNNER_AUTH_METHOD=token

# Docker Configuration
COMPOSE_PROJECT_NAME=vaultrunner
COMPOSE_FILE=docker-compose.yml

# Development settings
VAULTRUNNER_ENV=development